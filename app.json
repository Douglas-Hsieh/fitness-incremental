{
  "expo": {
    "name": "fitness-incremental",
    "slug": "fitness-incremental",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/male-avatar.png",
    "splash": {
      "image": "./assets/images/puppy.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.randompermutation.fitnessincremental",
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/male-avatar.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.randompermutation.fitnessincremental",
      "googleServicesFile": "./google-services.json",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "config": {
        "firebase": {
          "projectId": "fitness-incremental"
        }
      }
    },
    "plugins": [
      "react-native-health",
      "@react-native-google-signin/google-signin"
    ]
  }
}
